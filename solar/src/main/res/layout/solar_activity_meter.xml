<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">
    <include
        layout="@layout/common_toolbar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <!--编号-->
            <TextView
                android:text="编号"
                style="@style/TextView_Device_Value_Title"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv_code"
                    style="@style/TextView_Device_Code"
                    android:text="@string/unknown"
                    android:layout_weight="4"/>
                <TextView
                    android:id="@+id/tv_updateTime"
                    style="@style/TextView_Device_Code"
                    android:gravity="right"
                    android:text="@string/unknown"
                    android:layout_weight="3"
                    android:paddingRight="15dp"/>
            </LinearLayout>
            <!--功率-->
            <TextView
                style="@style/TextView_Device_Value_Title"
                android:text="参数"/>
            <LinearLayout
                style="@style/LinearLayout_Device_Content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="当前正向有功总电能"/>
                    <TextView
                        android:id="@+id/tv_PosActElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="总的有功功率"/>
                    <TextView
                        android:id="@+id/tv_ActivePower"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向有功尖时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardJElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向有功峰时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardFElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向有功平时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardPElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向有功谷时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardGElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="反向有功尖时电能"/>
                    <TextView
                        android:id="@+id/tv_DecBackwardJElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="反向有功峰时电能"/>
                    <TextView
                        android:id="@+id/tv_DecBackwardFElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="反向有功平时电能"/>
                    <TextView
                        android:id="@+id/tv_DecBackwardPElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="反向有功谷时电能"/>
                    <TextView
                        android:id="@+id/tv_DecBackwardGElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout><LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/primary_text_black"
                    android:textSize="@dimen/ts_lel_three"
                    android:text="正向无功尖时电能"/>
                <TextView
                    android:id="@+id/tv_DecFowardReactiveJElectric"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:textColor="@color/primary_text_black"
                    android:textSize="@dimen/ts_lel_three"
                    android:text="@string/unknown"/>
            </LinearLayout><LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/primary_text_black"
                    android:textSize="@dimen/ts_lel_three"
                    android:text="正向无功峰时电能"/>
                <TextView
                    android:id="@+id/tv_DecFowardReactiveFElectric"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:textColor="@color/primary_text_black"
                    android:textSize="@dimen/ts_lel_three"
                    android:text="@string/unknown"/>
            </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向无功平时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardReactivePElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="正向无功谷时电能"/>
                    <TextView
                        android:id="@+id/tv_DecFowardReactiveGElectric"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>