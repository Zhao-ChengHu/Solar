<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">
    <include
        layout="@layout/common_toolbar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <!--编号-->
            <TextView
                android:text="编号"
                style="@style/TextView_Device_Value_Title"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv_code"
                    style="@style/TextView_Device_Code"
                    android:text="@string/unknown"
                    android:layout_weight="4"/>
                <TextView
                    android:id="@+id/tv_updateTime"
                    style="@style/TextView_Device_Code"
                    android:gravity="right"
                    android:text="@string/unknown"
                    android:layout_weight="3"
                    android:paddingRight="15dp"/>
            </LinearLayout>
            <TextView
                android:text="运行"
                style="@style/TextView_Device_Value_Title"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    style="@style/TextView_Device_Code"
                    android:text="运行状态"
                    android:layout_weight="5"/>
                <TextView
                    android:id="@+id/tv_state"
                    style="@style/TextView_Device_Code"
                    android:gravity="right"
                    android:text="@string/unknown"
                    android:layout_weight="2"
                    android:paddingRight="15dp"/>
            </LinearLayout>
                />
            <!--电压-->
            <TextView
                style="@style/TextView_Device_Value_Title"
                android:text="参数"/>
            <LinearLayout
                style="@style/LinearLayout_Device_Content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="频率"/>
                    <TextView
                        android:id="@+id/tv_f"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="有功功率"/>
                    <TextView
                        android:id="@+id/tv_p"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="无功功率"/>
                    <TextView
                        android:id="@+id/tv_q"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="有功电镀"/>
                    <TextView
                        android:id="@+id/tv_ActPower"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="无功电镀"/>
                    <TextView
                        android:id="@+id/tv_ReactPower"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="开入量状态字3"/>
                    <TextView
                        android:id="@+id/tv_InputState3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="开入量状态字4"/>
                    <TextView
                        android:id="@+id/tv_InputState4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="开入量状态字5"/>
                    <TextView
                        android:id="@+id/tv_InputState5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:gravity="right"
                        android:maxLines="1"
                        android:textColor="@color/primary_text_black"
                        android:textSize="@dimen/ts_lel_three"
                        android:text="@string/unknown"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>