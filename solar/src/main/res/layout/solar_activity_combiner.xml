<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">
    <include
        layout="@layout/common_toolbar"/>
    <TextView
        style="@style/TextView_Device_Value_Title"
        android:text="编号"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_code"
            style="@style/TextView_Device_Code"
            android:text="@string/unknown"
            android:layout_weight="4"/>
        <TextView
            android:id="@+id/tv_updateTime"
            style="@style/TextView_Device_Code"
            android:gravity="right"
            android:text="@string/unknown"
            android:layout_weight="3"
            android:paddingRight="15dp"/>
    </LinearLayout>

    <TextView
        style="@style/TextView_Device_Value_Title"
        android:text="参数"/>
    <LinearLayout
        style="@style/LinearLayout_Device_Content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="电压"/>
            <TextView
                android:id="@+id/tv_voltage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="总电流"/>
            <TextView
                android:id="@+id/tv_TotalCurrent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="外部温度1"/>
            <TextView
                android:id="@+id/tv_ExternalTemp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="反向总电流"/>
            <TextView
                android:id="@+id/tv_TotalInverseCurrent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="漏电流数值"/>
            <TextView
                android:id="@+id/tv_LeakageCurrent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="电流通道总功率"/>
            <TextView
                android:id="@+id/tv_TotalPower"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
    </LinearLayout>

    <TextView
        style="@style/TextView_Device_Value_Title"
        android:text="节点设备"/>
    <LinearLayout
        style="@style/LinearLayout_Device_Content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="母线电压"/>
            <TextView
                android:id="@+id/tv_NoteBusVoltage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="外部温度"/>
            <TextView
                android:id="@+id/tv_NoteExTemp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="电流通道数"/>
            <TextView
                android:id="@+id/tv_NoteChannelCount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="无线节点距离"/>
            <TextView
                android:id="@+id/tv_NoteWirelessDistance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="无线节点连接数量"/>
            <TextView
                android:id="@+id/tv_WirelessConnectCount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:textColor="@color/primary_text_black"
                android:textSize="@dimen/ts_lel_three"
                android:text="@string/unknown"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>